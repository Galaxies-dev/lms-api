      "mappedBy": "course"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "isPremium": {
      "type": "boolean",
      "default": false
    },
    "slug": {
      "type": "uid"
    },
    "revenuecatId": {
      "type": "string"
    }
  }
